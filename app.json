{
  "expo": {
    "name": "welcom",
    "slug": "welcom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashScreen.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash":{
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/react-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
